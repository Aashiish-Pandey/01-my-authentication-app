ers: {\n        \"Content-Type\": \"application/json\",\n      },\n    }).then((res) => {\n      setIsLoading(false);\n      if (res.ok) {\n\n        return res.json()\n        //...\n      } else {\n        return res.json().then((data) => {\n          //show an error model\n          let errorMessage = \"Authentication failed\";\n          if (data && data.error && data.error.message) {\n            errorMessage = data.error.message;\n          }\n          \n          throw new Error(errorMessage)\n        });\n      }\n    }).then(data=>{authCtx.login(data.idToken)}).catch(err=> {\n\n      alert(err.message);\n\n    })\n  };\n\n  return (\n    <section className={classes.auth}>\n      <h1>{isLogin ? \"Login\" : \"Sign Up\"}</h1>\n      <form onSubmit={submitHandler}>\n        <div className={classes.control}>\n          <label htmlFor=\"email\">Your Email</label>\n          <input type=\"email\" id=\"email\" ref={emailInputRef} required />\n        </div>\n        <div className={classes.control}>\n          <label htmlFor=\"password\">Your Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            ref={passwordInputRef}\n            required\n          />\n        </div>\n        <div className={classes.actions}>\n          <button>{isLogin ? \"Login\" : \"Create Account\"}</button>\n          <button\n            type=\"button\"\n            className={classes.toggle}\n            onClick={switchAuthModeHandler}\n          >\n            {isLogin ? \"Create new account\" : \"Login with existing account\"}\n          </button>\n        </div>\n      </form>\n    </section>\n  );\n};\n\nexport default AuthForm;\n","C:\\Users\\amitk\\OneDrive\\Desktop\\01-starting-project\\01-my-authentication-app\\src\\components\\Profile\\ProfileForm.js",[],"C:\\Users\\amitk\\OneDrive\\Desktop\\01-starting-project\\01-my-authentication-app\\src\\store\\auth-context.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replaced